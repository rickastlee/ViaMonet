<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <solid
                android:color="@android:color/transparent" />
            <corners
                android:radius="@dimen/bk" />
            <stroke
                android:width="@dimen/r"
                android:color="@android:color/black" />
        </shape>
    </item>
    <item>
        <inset
            android:insetTop="@dimen/p"
            android:insetBottom="0.0dip">
            <shape>
                <solid
                    android:color="@android:color/transparent" />
                <corners
                    android:bottomLeftRadius="@dimen/bk"
                    android:bottomRightRadius="@dimen/bk" />
                <stroke
                    android:width="@dimen/r"
                    android:color="@android:color/black" />
            </shape>
        </inset>
    </item>
</layer-list>